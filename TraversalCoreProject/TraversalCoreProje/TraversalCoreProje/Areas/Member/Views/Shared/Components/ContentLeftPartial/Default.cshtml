@model List<About2>
<div class="col-xl-4 col-lg-12">
    <div class="card">
        @foreach(var x in Model){
        <div class="card-header">
            <h4 class="card-title" id="heading-multiple-thumbnails">@x.Title</h4>
            <a class="heading-elements-toggle">
                <i class="la la-ellipsis-v font-medium-3"></i>
            </a>
            <div class="heading-elements">
                <span class="avatar">

                    <img src="~/member3/theme-assets/images/portrait/small/avatar-s-2.png" alt="avatar">
                </span>
                <span class="avatar">
                    <img src="~/member3/theme-assets/images/portrait/small/avatar-s-3.png" alt="avatar">
                </span>
                <span class="avatar">
                    <img src="~/member3/theme-assets/images/portrait/small/avatar-s-4.png" alt="avatar">
                </span>
            </div>
        </div>
        <div class="card-content">
            <div class="card-body">
                <h4 class="card-title">@x.Title2</h4>
                <p class="card-text">
                @x.Description
                </p>
            </div>
        </div>
        }
    </div>
</div>